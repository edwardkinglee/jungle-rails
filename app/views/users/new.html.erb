<h1>Sign Up!</h1>

<%= form_for :user, url: '/users' do |f| %>
  <p>First Name: <%= f.text_field :first_name %></p>
  <p>Last Name: <%=f.text_field :last_name %></p>
  <p>Email:&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<%= f.text_field :email %></p>
  Password:&nbsp&nbsp&nbsp<%= f.password_field :password %>
  Password Confirmation:<%= f.password_field :password_confirmation %>
  <%= f.submit "Submit" %>

<% end %>